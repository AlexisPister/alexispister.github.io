@article{dangTimeArcsVisualizingFluctuations2016,
  title = {{{TimeArcs}}: {{Visualizing Fluctuations}} in {{Dynamic Networks}}},
  shorttitle = {{{TimeArcs}}},
  author = {Dang, T. N. and Pendar, N. and Forbes, A. G.},
  date = {2016-06},
