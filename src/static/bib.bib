@article{rollingerCicerosSupplicatioUnd2017,
  title = {Ciceros supplicatio und aristokratische Konkurrenz im Senat der Späten Republik},
  author = {Rollinger, Christian},
  date = {2017-08-21},
  journaltitle = {Klio},
  volume = {99},
  number = {1},
  pages = {192--225},
  publisher = {{De Gruyter (A)}},
  issn = {2192-7669},
  doi = {10.1515/klio-2017-0007},
  url = {https://www.degruyter.com/document/doi/10.1515/klio-2017-0007/html},
  urldate = {2022-07-11},
  abstract = {Zusammenfassung: Ciceros Wunsch nach einer supplicatio im Anschluss an sein kilikisches Prokonsulat ist bislang, von wenigen Ausnahmen abgesehen, kaum Beachtung geschenkt worden, die über eine gelegentliche Skizze im Rahmen biographischer Abhandlungen hinausgekommen wäre. Dabei zeigt aber gerade die Korrespondenz Ciceros während und unmittelbar nach seinem Prokonsulat sehr eindringlich, wie heftig der inneraristokratische Kampf um Auszeichnung und Prestige geführt wurde und bietet uns einen zeitgenössischen Einblick in die Mechaniken dieses Wettstreites, wie er informell im persönlichen und brieflichen Umgang und formell in den Sitzungen des Senates ausgetragen wurde. Ciceros Briefverkehr illustriert, wie komplexe Geflechte aus Verbindlichkeiten und gelegentlichen Abneigungen bei Senatsdebatten und Entscheidungen über öffentliche Auszeichnungen zum Tragen kamen und welche Mittel den Betroffenen zur Durchsetzung ihres Anliegens offenstanden. Somit geben sie uns auch einen wertvollen Blick auf die Bedeutung aristokratischen Prestiges, dessen standesinterne Verhandlung und die politische Praxis am Ende der Republik frei.},
  langid = {ngerman},
  keywords = {Cicero,HSNA,Kilikien,Nobilität,Senat,Späte Republik,unipartite}
}

