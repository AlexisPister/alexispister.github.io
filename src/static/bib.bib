@article{carberryDiffLensDifferencedrivenVisualization,
  title = {{{DiffLens}}: {{Difference-driven Visualization}} for {{Graph Sampling Evaluation}}},
  author = {Carberry, Josiah S and Grimley, Ed and Stewart, Martha},
  abstract = {A variety of graph sampling algorithms have been proposed to simplify graph computation and visualization. Despite their specific goals, it is inevitable to filter out a large amount of nodes and edges, thus resulting in structural differences between the sampled and original graphs. Many metrics have been proposed to represent the network structures from different perspectives such as node degree, shortest path and clustering coefficients. Although the value changes of these metrics can reveal the overall statistical differences between the sampled and original graphs, it is still difficult to investigate the detailed distributions of such differences and the underlying reasons for them, which is crucial for accurately evaluating the effectiveness of graph sampling algorithms. In this paper, we propose DiffLens, a difference-driven visualization approach for graph sampling evaluation. First, we thoroughly survey the existing graph evaluation metrics, and classify them into three categories (i.e., neighbor-based, path-based, and structure-based). Then, to avoid investigating every graph evaluation metric, we conduct extensive statistical analysis on different graph datasets to select the most representative metrics for each category. Building upon this, we further propose novel quantification strategies to measure each category of graph differences. Finally, we design a multicolor heatmap to visually present the primary differences across different local areas of a graph, and provide a variety of coordinated views to facilitate the exploration of the underlying reasons for such differences caused by different graph sampling algorithms. Case studies using real-world networks and in-depth expert interviews are conducted to demonstrate the effectiveness and usability of DiffLens in helping graph sampling algorithm developers and users evaluate and compare different graph sampling algorithms.},
  langid = {english},
  file = {/home/alexis/Zotero/storage/QQPU8ET7/Carberry et al. - DiffLens Difference-driven Visualization for Grap.pdf}
}

